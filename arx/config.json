{
  "operations": ["suppress", "pseudonymize", "k_anonymize"],
  "dataset_name": "Medical_Data_new.csv",
  "data_type": "medical",
  "medical": {
    "suppress": ["Date of Birth", "Address"],
    "pseudonymize": ["Name", "Patient ID"],
    "generalize": ["Age", "PIN Code", "Height", "Weight"],
    "insensitive_columns": [
      "S.No",
      "Patient ID",
      "Name",
      "Gender",
      "Test Result",
      "Blood Pressure"
    ],

    "k_anonymize": {
      "k": 50
    },
    "allow_record_suppression": "True"
  }
}
