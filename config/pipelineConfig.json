{
  "data_file": "../data/sampleHealthData_positivity.json",
  "data_type": "",
  "medical": {
    "suppress": ["Date of Birth", "Address", "Lab Results"],
    "pseudonymize": ["Name", "Patient ID"],
    "k_anonymize": {
      "generalize": "Age",
      "k": 500,
      "min_bin_value": 0,
      "max_bin_value": 101
    },
    "differential_privacy": {
      "dp_query": "mean",
      "dp_output_attribute": "Days To Negative",
      "dp_selected_output": "Positive",
      "dp_aggregate_attribute": "Gender",
      "dp_max_value_output_attribute": 50,
      "dp_epsilon": 0.1,
      "dp_epsilon_step": 0.1
    }
  },
  "spatioTemporal": {
    "suppress": [
      "trip_direction",
      "last_stop_arrival_time",
      "route_id",
      "actual_trip_start_time",
      "trip_delay",
      "vehicle_label",
      "last_stop_id",
      "id",
      "location.type"
    ],
    "pseudonymize": ["license_plate", "trip_id"],
    "spatial_generalize": {
      "spatial_attribute": "location.coordinates",
      "h3_resolution": 7,
      "filter_attribute": "license_plate",
      "filter_event_occurences": 30,
      "filter_attribute_by": ["HAT", "Date"]
    },
    "temporal_generalize": {
      "temporal_attribute": "observationDateTime",
      "start_time": 9,
      "end_time": 20
    },
    "differential_privacy": {
      "dp_query": "mean",
      "dp_output_attribute": "speed",
      "dp_aggregate_attribute": ["HAT", "Date", "license_plate"],
      "dp_query_value_threshold": 60,
      "dp_epsilon": 1,
      "dp_epsilon_step": 0.1,
      "global_max_value": 65,
      "global_min_value": 0
    }
  }
}
