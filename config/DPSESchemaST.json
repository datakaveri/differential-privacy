{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "dataFile": {
      "type": "string"
    },
    "genType": {
      "type": "string"
    },
    "spatio-temporal": {
      "type": "object",
      "properties": {
        "duplicateDetection": {
          "type": "array",
          "maxItems": 2,
          "minItems": 2,
          "uniqueItems": true,
          "items": [
            {
              "type": "string"
            }
          ]
        },
        "suppressCols": {
          "type": "array",
          "items": [
            {
              "type": "string"
            }
          ]
        },
        "locationCol": {
          "type": "string"
        },
        "datetimeCol": {
          "type": "string"
        },
        "groupByCol": {
          "type": "string"
        },
        "trueValue": {
          "type": "string"
        },
        "localityFactor": {
          "maximum": 1,
          "minimum": 0,
          "type": "number"
        },
        "winsorizeLowerBound": {
          "maximum": 0.1,
          "minimum": 0.01,
          "type": "number"
        },
        "winsorizeUpperBound": {
          "maximum": 1.0,
          "minimum": 0.9,
          "type": "number"
        },
        "globalMaxValue": {
          "maximum": 200,
          "minimum": 0,
          "type": "integer"
        },
        "globalMinValue": {
          "maximum": 200,
          "minimum": 0,
          "type": "integer"
        },
        "privacyLossBudgetEpsQuery": {
          "type": "array",
          "items": {
            "type": "number"  
          },
          "maximum": 100,
          "minimum": 0
        },
        "h3Resolution": {
          "minimum": 6,
          "maximum": 8,
          "type": "integer"
        },
        "startTime": {
          "minimum": 0,
          "maximum": 23,
          "type": "integer"
        },
        "endTime": {
          "minimum": 0,
          "maximum": 23,
          "type": "integer"
        },
        "minEventOccurences": {
          "type": "integer"
        },
        "trueValueThreshold": {
          "type": "integer"
        },
        "snrUpperLimit": {
          "type": "integer"
        },
        "snrLowerLimit": {
          "type": "integer"
        }
      },
      "required": [
        "duplicateDetection",
        "suppressCols",
        "locationCol",
        "datetimeCol",
        "groupByCol",
        "localityFactor",
        "winsorizeLowerBound",
        "winsorizeUpperBound",
        "globalMaxValue",
        "globalMinValue",
        "privacyLossBudgetEpsQuery",
        "h3Resolution",
        "startTime",
        "endTime",
        "minEventOccurences",
        "trueValueThreshold",
        "snrLowerLimit",
        "snrUpperLimit"
      ]
    }
  },
  "required": [
    "dataFile"]
}
